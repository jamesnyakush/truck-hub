<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_default"
    app:startDestination="@id/main">

    <fragment
        app:moduleName="Home"
        android:id="@+id/main"
        android:name="com.jamesnyakush.carhub.home.ui.Home"
        android:label="Home"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/payment_fragment"
        android:name="com.jamesnyakush.carhub.payment.ui.Payment"
        android:label="Payment"
        app:moduleName="Payment"
        tools:layout="@layout/payment_fragment" />
    <fragment
        android:id="@+id/favourite_fragment"
        android:name="com.jamesnyakush.carhub.favourite.ui.FavouriteFragment"
        android:label="Favourite"
        app:moduleName="Favourite"
        tools:layout="@layout/favourite_fragment" />
    <fragment
        android:id="@+id/history_fragment"
        android:name="com.jamesnyakush.carhub.history.ui.History"
        android:label="History"
        app:moduleName="History"
        tools:layout="@layout/history_fragment" />

</navigation>